{
  "name": "flownow-backend",
  "version": "1.0.0",
  "description": "Backend for FlowNow meditation and focus training app",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "meditation",
    "focus",
    "training",
    "graphql",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/experimental-nextjs-app-support": "^0.12.2",
    "@apollo/server": "^4.9.5",
    "@graphql-tools/schema": "^10.0.0",
    "@supabase/supabase-js": "^2.39.3",
    "apollo-server-express": "^3.12.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "graphql": "^16.11.0",
    "graphql-ws": "^5.14.2",
    "jwt-decode": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "type-graphql": "^2.0.0-rc.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/pdf-parse": "^1.1.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
